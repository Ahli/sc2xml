    lv_i = 1;
    for ( ; ( (autoAC7A6831_ai >= 0 && lv_i <= autoAC7A6831_ae) || (autoAC7A6831_ai < 0 && lv_i >= autoAC7A6831_ae) ) ; lv_i += autoAC7A6831_ai ) {
