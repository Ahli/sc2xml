//--------------------------------------------------------------------------------------------------
// Trigger: Victory Scene
//--------------------------------------------------------------------------------------------------
bool gt_VictoryScene_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unitgroup lv_vultures;

    // Automatic Variable Declarations
    unitgroup auto3A1CB0E6_g;
    int auto3A1CB0E6_u;
    unit auto3A1CB0E6_var;
    unitgroup auto2E28554E_g;
    int auto2E28554E_u;
    unit auto2E28554E_var;

    // Variable Initialization
    lv_vultures = UnitGroupEmpty();

    // Actions
    if (!runActions) {
        return true;
    }

    UnitGroupIssueOrder(gv_victoryRaynorsUnits, Order(AbilityCommand("stop", 0)), c_orderQueueReplace);
    Wait(0.5, c_timeGame);
    auto3A1CB0E6_g = gv_victoryOrlansUnits;
    auto3A1CB0E6_u = UnitGroupCount(auto3A1CB0E6_g, c_unitCountAll);
    for (;; auto3A1CB0E6_u -= 1) {
        auto3A1CB0E6_var = UnitGroupUnitFromEnd(auto3A1CB0E6_g, auto3A1CB0E6_u);
        if (auto3A1CB0E6_var == null) { break; }
        libNtve_gf_PauseUnit(auto3A1CB0E6_var, true);
        Wait(0.25, c_timeGame);
    }
    libNtve_gf_SetAlliance(gv_p01_USER, gv_p02_ORLAN_ENEMY, libNtve_ge_AllianceSetting_Neutral);
    Wait(0.5, c_timeGame);
    auto2E28554E_g = UnitGroup("Vulture", gv_p01_USER, RegionFromId(62), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0);
    auto2E28554E_u = UnitGroupCount(auto2E28554E_g, c_unitCountAll);
    for (;; auto2E28554E_u -= 1) {
        auto2E28554E_var = UnitGroupUnitFromEnd(auto2E28554E_g, auto2E28554E_u);
        if (auto2E28554E_var == null) { break; }
        UnitGroupAdd(lv_vultures, auto2E28554E_var);
        UnitSetPropertyFixed(auto2E28554E_var, c_unitPropMovementSpeed, 0.25);
    }
    Wait(3.0, c_timeGame);
    UnitIssueOrder(UnitGroupUnit(lv_vultures, 2), OrderTargetingUnit(AbilityCommand("move", 0), UnitFromId(90)), c_orderQueueReplace);
    Wait(0.5, c_timeGame);
    UnitIssueOrder(UnitGroupUnit(lv_vultures, 4), OrderTargetingUnit(AbilityCommand("move", 0), UnitFromId(90)), c_orderQueueReplace);
    Wait(0.5, c_timeGame);
    UnitIssueOrder(UnitGroupUnit(lv_vultures, 5), OrderTargetingUnit(AbilityCommand("move", 0), UnitFromId(90)), c_orderQueueReplace);
    return true;
}

