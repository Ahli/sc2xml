//--------------------------------------------------------------------------------------------------
// Trigger: Tip - Moebius Artifact Note
//--------------------------------------------------------------------------------------------------
bool gt_TipMoebiusArtifactNote_Func (bool testConds, bool runActions) {
    // Variable Declarations
    bool lv_displayTip;

    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((TriggerIsEnabled(TriggerGetCurrent()) == true))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    TriggerEnable(TriggerGetCurrent(), false);
    if ((gv_labResearch_ConvertProtoss >= 0) && (gv_labResearch_ConvertZerg >= 0)) {
        libCamp_gf_SetStoryState(libCamp_ge_StoryStateID_StoryStateTipVisitLabPlayed, 1);
        if ((ConversationDataStateGetValue("StoryTips|MoebiusArtifactNote") == 0) && (ConversationDataStateGetValue("StoryTips|MoebiusArtifactNote2") == 0)) {
            ConversationDataStateSetValue("StoryTips|MoebiusArtifactNote2", 1);
            gf_AddTip("StoryTips|MoebiusArtifactNote");
            return true;
        }

        if ((ConversationDataStateGetValue("StoryTips|MoebiusArtifactNote") == 0)) {
            gf_AddTip("StoryTips|MoebiusArtifactNote");
            return true;
        }

        if ((ConversationDataStateGetValue("StoryTips|MoebiusArtifactNote2") == 0)) {
            gf_AddTip("StoryTips|MoebiusArtifactNote2");
            return true;
        }

    }

    if ((gv_labResearch_ConvertProtoss >= 0)) {
        libCamp_gf_SetStoryState(libCamp_ge_StoryStateID_StoryStateTipVisitLabPlayed, 1);
        gf_AddTip("StoryTips|MoebiusArtifactNote");
        return true;
    }

    if ((gv_labResearch_ConvertZerg >= 0)) {
        libCamp_gf_SetStoryState(libCamp_ge_StoryStateID_StoryStateTipVisitLabPlayed, 1);
        gf_AddTip("StoryTips|MoebiusArtifactNote2");
        return true;
    }

    return true;
}

