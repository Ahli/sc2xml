//--------------------------------------------------------------------------------------------------
// Trigger: Init 06 Intro
//--------------------------------------------------------------------------------------------------
bool gt_Init06Intro_Func (bool testConds, bool runActions) {
    // Actions
    if (!runActions) {
        return true;
    }

    if ((gv_isTransitionalLoad == true)) {
        TriggerExecute(gt_ReturnGeneric2, true, true);
        return true;
    }

    if ((GameIsTestMap(true) == true)) {
        TriggerExecute(gt_DebugAutomatedTesting, true, false);
        return true;
    }

    if ((libCamp_gf_LastMap() == libCamp_ge_MapID_MapNone)) {
        gf_DisplayMissionDebriefing(libCamp_ge_MapID_MapNone);
        return true;
    }

    if ((libCamp_gf_LastMap() == libCamp_ge_MapID_MapTArcade)) {
        TriggerExecute(gt_ReturnToCantinaArcade, true, false);
        return true;
    }

    if ((libCamp_gf_LastMap() == libCamp_ge_MapID_MapTSimulator)) {
        TriggerExecute(gt_ReturnToSimulator, true, false);
        return true;
    }

    if ((libCamp_gf_MapTypeCheck(libCamp_gf_LastMap(), libCamp_ge_MapType_Mission) == false)) {
        TriggerExecute(gt_ReturnGeneric, true, false);
        return true;
    }

    if (((libCamp_gf_LastMissionConditionCheck(libCamp_ge_MissionCondition_Simulation) == true) || (libCamp_gf_MissionStatusCheck(libCamp_ge_MapID_MapTValerian03, libCamp_ge_MissionStatus_Completed) == true))) {
        TriggerExecute(gt_ReturnToBattleReport, true, false);
        return true;
    }

    if ((libCamp_gf_LastMissionConditionCheck(libCamp_ge_MissionCondition_Successful) == false)) {
        TriggerExecute(gt_ReturnGeneric, true, false);
        return true;
    }

    gf_DisplayMissionDebriefing(libCamp_gf_LastMission());
    return true;
}

