    auto75C0B0D3_g = lv_units;
    auto75C0B0D3_u = 1;
    for ( ; auto75C0B0D3_u <= UnitGroupCount(auto75C0B0D3_g, c_unitCountAll) ; auto75C0B0D3_u += BoolToInt(lv_unit == UnitGroupUnit(auto75C0B0D3_g, auto75C0B0D3_u)) ) {
