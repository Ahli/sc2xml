    }

    gv_terranUpTheNightPlaying = true;
    if ((gv_jukeboxes[gv_marSaraJukeboxID].lv_playback.lv_muted == false) && (gv_jukeboxes[gv_marSaraJukeboxID].lv_playback.lv_music != null)) {
