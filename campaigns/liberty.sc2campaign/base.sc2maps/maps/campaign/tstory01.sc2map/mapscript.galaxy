        libNtve_gf_ClearAnimation(libNtve_gf_MainActorofUnit(lp_unit), gv_aNIMID_HIGHLIGHT);
        libNtve_gf_ClearAnimation(libNtve_gf_MainActorofUnit(lp_unit), gv_aNIMID_SHEEN);
    }
}

