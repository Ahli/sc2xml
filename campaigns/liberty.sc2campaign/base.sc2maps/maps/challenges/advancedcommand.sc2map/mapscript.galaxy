//--------------------------------------------------------------------------------------------------
bool gt_CreateMultikillObject_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!((UnitGetOwner(EventUnit()) != 1))) {
            return false;
        }

        if (!((UnitGetType(EventUnit()) != "MultiKillObject"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    libNtve_gf_CreateUnitsWithDefaultFacing(1, "MultiKillObject", 0, 1, UnitGetPosition(EventUnit()));
    gv_multiKillLastKillPosition = UnitGetPosition(EventUnit());
    return true;
}

