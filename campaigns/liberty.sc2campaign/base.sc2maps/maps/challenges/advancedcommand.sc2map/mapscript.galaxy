//--------------------------------------------------------------------------------------------------
// Trigger: Intro Group Cuts
//--------------------------------------------------------------------------------------------------
bool gt_IntroGroupCuts_Func (bool testConds, bool runActions) {
    // Variable Declarations
    fixed lv_totalDuration;
    fixed lv_individualDuration;

    // Variable Initialization

    // Actions
    if (!runActions) {
        return true;
    }

    lv_totalDuration = (SoundLengthSync(SoundLink("ChallengescombinedExecutor00035", -1)) + SoundLengthSync(SoundLink("ChallengescombinedExecutor00036", -1)));
    lv_individualDuration = (lv_totalDuration / 5.0);
    CameraApplyInfo(gv_pLAYER_USER, CameraInfoFromId(1703), 0.0, -1, 10, true);
    Wait(lv_individualDuration, c_timeReal);
    CameraApplyInfo(gv_pLAYER_USER, CameraInfoFromId(1704), 0.0, -1, 10, true);
    Wait(lv_individualDuration, c_timeReal);
    CameraApplyInfo(gv_pLAYER_USER, CameraInfoFromId(1705), 0.0, -1, 10, true);
    Wait(lv_individualDuration, c_timeReal);
    CameraApplyInfo(gv_pLAYER_USER, CameraInfoFromId(248), 0.0, -1, 10, true);
    Wait(lv_individualDuration, c_timeReal);
    CameraApplyInfo(gv_pLAYER_USER, CameraInfoFromId(1706), 0.0, -1, 10, true);
    return true;
}

