            libVCUI_gf_SetProgressBarLabel((StringExternal("Param/Value/75784CD7") + FixedToText((100.0 * (gv_s2_KerriganPowerProgress / gv_s2_KerriganPowerProgressMax)), 1) + StringExternal("Param/Value/C93EBE22")));
