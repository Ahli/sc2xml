        Wait(1.0, c_timeGame);
        gv_s2_KerriganPowerProgress = TimerGetElapsed(gv_s2_ProtectKerriganTimer);
        libVCUI_gf_SetProgressBarProgress(FixedToInt((100.0 * (gv_s2_KerriganPowerProgress / gv_s2_KerriganPowerProgressMax))));
