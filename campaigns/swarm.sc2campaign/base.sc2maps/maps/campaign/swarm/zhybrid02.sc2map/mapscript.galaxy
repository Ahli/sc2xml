        libNtve_gf_CreateExplosionAtPoint(libNtve_ge_ExplosionSize_Small, libNtve_ge_ExplosionType_Terran, RegionRandomPoint(RegionFromId(61)));
        Wait(0.05, c_timeReal);
    }
    return true;
}

