//==================================================================================================
//  Terran Melee Hard - Very Hard
//==================================================================================================

// very similar to Very Hard builds, but with minor deliberate delays/weakness

// these builds are based on Swarm builds, but with only worker counts updated
// this makes them weaker then the newly optimized builds in VyHd which is the goal

bool TerranOpenRushMarineHdVH (int player);                   // heavy early marine rush 
bool TerranOpenRushMarineReaperHdVH (int player);             // marine w/ early reaper harass
bool TerranOpenRushProxyBarracksHdVH (int player);            // proxy barracks marine rush
bool TerranOpenTimingMMStimHdVH (int player);                 // stim timing push
bool TerranOpenTimingHellionMarauderHdVH (int player);        // hellion marauder timing push
bool TerranOpenTimingMariSiegeHdVH (int player);              // marine + siege push
bool TerranOpenTimingCloakBansheeHdVH (int player);           // cloak banshee timing
bool TerranOpenPowerMacroQuickExpGndHdVH (int player);        // aggresive quick expand
bool TerranOpenPowerMacroMarineExpGndHdVH (int player);       // gasless marines expand
bool TerranOpenPowerMacroDefenseExpGndHdVH (int player);      // safer marine/siege expand
bool TerranOpenAirOneBaseAirHdVH (int player);                // single base "island" air build
bool TerranOpenAirOneBaseDropHdVH (int player);               // single base "island" drop build
bool TerranOpenAirCloakBansheeHdVH (int player);              // single base "island" cloak banshee
bool TerranOpenAirQuickExpandAirHdVH (int player);            // quick expand air build

bool TerranMidRushTimingMMMedivacHdVH (int player);           // continued agression with mmm
bool TerranMidRushTimingDefensiveExpandHdVH (int player);     // transition from early pressure to a macro game
bool TerranMidPowerMMMedivacHdVH (int player);                // standard bio attack
bool TerranMidPowerMMSiegeHdVH (int player);                  // bio+siege
bool TerranMidPowerSiegeBansheeHdVH (int player);             // delayed 1,1,1
bool TerranMidPowerHellionSiegeHdVH (int player);             // low-tech mech
bool TerranMidPowerSiegeThorHdVH (int player);                // high-tech mech
bool TerranMidMacroBioHdVH (int player);                      // macro toward late game bio
bool TerranMidMacroMechHdVH (int player);                     // macro toward late game mech
bool TerranMidMacroThorBCHdVH (int player);                   // macro toward late game thor/bc
bool TerranMidAirOneBaseAirHdVH (int player);                 // continue single base "island" air build
bool TerranMidAirOneBaseDropHdVH (int player);                // continue single base "island" drop build
bool TerranMidAirExpansionVikingBansheeHdVH (int player);     // quick expand viking/banshee attack
bool TerranMidAirExpansionBCHdVH (int player);                // quick expand battlecruiser attack
bool TerranMidAirExpansionDropAirHdVH (int player);           // quick expand drop + air attack
bool TerranMidAirMacroHdVH (int player);                      // macro toward a large late game air force

//--------------------------------------------------------------------------------------------------
//  TerranOpenHdVH
//--------------------------------------------------------------------------------------------------
void TerranOpenHdVH (int player) {
    bool done = false;
    int buildIndex;

    AIEarlyDefenseScouting(player);

    // see if we need to switch builds
    UpdateHDTerranOpening(player);
    
    buildIndex = AIGetUserInt(player, c_currentBuildIndex);
    if (buildIndex == e_hdBuildIndex_TOpenRush_Marine)                      { done = TerranOpenRushMarineHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenRush_MarineReaper)           { done = TerranOpenRushMarineReaperHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenRush_ProxyBarracks)          { done = TerranOpenRushProxyBarracksHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenTiming_MMStim)               { done = TerranOpenTimingMMStimHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenTiming_HellionMarauder)      { done = TerranOpenTimingHellionMarauderHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenTiming_MariSiege)            { done = TerranOpenTimingMariSiegeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenTiming_CloakBanshee)         { done = TerranOpenTimingCloakBansheeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenPowerMacro_QuickExpGnd)      { done = TerranOpenPowerMacroQuickExpGndHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenPowerMacro_MarineExpGnd)     { done = TerranOpenPowerMacroMarineExpGndHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenPowerMacro_DefenseExpGnd)    { done = TerranOpenPowerMacroDefenseExpGndHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenAir_OneBaseAir)              { done = TerranOpenAirOneBaseAirHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenAir_OneBaseDrop)             { done = TerranOpenAirOneBaseDropHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenAir_CloakBanshee)            { done = TerranOpenAirCloakBansheeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TOpenAir_QuickExpandAir)          { done = TerranOpenAirQuickExpandAirHdVH (player); }
    else { ErrorMeleeScript(player, "Invalid Open BuildIndex TerranHdVH"); }

    if (done) {
        TerranHDInitMid(player); // Transition to MidGame
    }
}

//--------------------------------------------------------------------------------------------------
//  TerranMidHdVH
//--------------------------------------------------------------------------------------------------
void TerranMidHdVH (int player) {
    bool done = false;
    int buildIndex;
    
    // enable scouting as appropriate
    if (AIGetUserInt(player, c_numGroundStartLocs) > 0) {
        AIScoutingEnable(player, 15, c_TU_SCV);
        AIEarlyDefenseScouting(player);
    }
    else {
        AIScoutingEnable(player, 1, c_TU_Medivac);
    }

    // see if we need to switch builds
    UpdateHDTerranMidGame(player);
    
    buildIndex = AIGetUserInt(player, c_currentBuildIndex);
    if (buildIndex == e_hdBuildIndex_TMidRushTiming_MMMedivac)              { done = TerranMidRushTimingMMMedivacHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidRushTiming_DefensiveExpand)   { done = TerranMidRushTimingDefensiveExpandHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidPower_MMMedivac)              { done = TerranMidPowerMMMedivacHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidPower_MMSiege)                { done = TerranMidPowerMMSiegeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidPower_SiegeBanshee)           { done = TerranMidPowerSiegeBansheeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidPower_HellionSiege)           { done = TerranMidPowerHellionSiegeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidPower_SiegeThor)              { done = TerranMidPowerSiegeThorHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidMacro_Bio)                    { done = TerranMidMacroBioHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidMacro_Mech)                   { done = TerranMidMacroMechHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidMacro_ThorBC)                 { done = TerranMidMacroThorBCHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_OneBaseAir)               { done = TerranMidAirOneBaseAirHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_OneBaseDrop)              { done = TerranMidAirOneBaseDropHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_ExpansionVikingBanshee)   { done = TerranMidAirExpansionVikingBansheeHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_ExpansionBC)              { done = TerranMidAirExpansionBCHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_ExpansionDropAir)         { done = TerranMidAirExpansionDropAirHdVH (player); }
    else if (buildIndex == e_hdBuildIndex_TMidAir_Macro)                    { done = TerranMidAirMacroHdVH (player); }
    else { ErrorMeleeScript(player, "Invalid Mid BuildIndex TerranHdVH"); }

    if (done) {
        TerranHDInitLate(player); // Transition to LateGame
    }
}

