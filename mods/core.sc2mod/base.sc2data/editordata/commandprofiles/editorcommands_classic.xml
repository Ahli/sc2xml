<?xml version="1.0" encoding="us-ascii"?>
<EditorCommands>
    <!-- Gizmos -->
    <cmd label="GizmoNone">                 <key>Q</key>    <mods>CA</mods> </cmd>
    <cmd label="GizmoTranslate">            <key>W</key>    <mods>CA</mods> </cmd>
    <cmd label="GizmoRotate">               <key>E</key>    <mods>CA</mods> </cmd>
    <cmd label="GizmoScale">                <key>R</key>    <mods>CA</mods> </cmd>
    <cmd label="GizmoNormalMode">           <key>Y</key>    <mods>CA</mods> </cmd>
    
    <!-- Commands to avoid being superceded by gizmos -->
    <cmd label="BrushTypeNext">             <key>Y</key>                    </cmd>
    <cmd label="SelectChoose">              <key>Q</key>                    </cmd>
    <cmd label="TerrainToggleWater">        <key>W</key>                    </cmd>
    <cmd label="TerrainToggleParticles">    <key>E</key>                    </cmd>
    <cmd label="TerrainLayerRegions">       <key>R</key>                    </cmd>

    <!-- Cutscene Editor -->
    <cmd label="PreviewerToggleFiles">      <key>F</key>                    </cmd>
    <cmd label="PreviewerZoomToFit">        <key>F</key>    <mods>S</mods>  </cmd>
</EditorCommands>
