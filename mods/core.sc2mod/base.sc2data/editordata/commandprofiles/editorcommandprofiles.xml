<?xml version="1.0" encoding="us-ascii"?>
<EditorCommandProfiles>
    <!-- ChoiceVersion
         Used to determine when to prompt users to choose a profile. Increase this when
         adding/removing profiles, or significantly changing their contents. This will cause
         all users who haven't yet chosen a profile at the new version level to be prompted again.
    -->
    <ChoiceVersion value="1"/>
    
    <!-- Profiles
         A corresponding EditorCommands_<id>.xml file should contain the command definitions.
    -->
    <Profile id="Classic" name="EDSTR_COMMANDPROFILE_CLASSIC"/>
    <Profile id="Modern" name="EDSTR_COMMANDPROFILE_MODERN"/>
</EditorCommandProfiles>
