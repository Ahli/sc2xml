bool ZergOpenPowerMacroQuickExpGndHdVH (int player);         // 1v1 quick ground expansion
bool ZergOpenPowerMacroDefenseExpGndHdVH (int player);       // team game defensive ground expansion
bool ZergOpenAirOneBaseMutaHdVH (int player);                // single base "island" muta build
bool ZergOpenAirOneBaseDropHdVH (int player);                // single base "island" drop build
bool ZergOpenAirQuickExpandAirHdVH (int player);             // quick expand air build

bool ZergMidRushTimingLingBaneRoachHdVH (int player);        // continue the early pressure with ling/bane/roach
bool ZergMidRushTimingDefensiveExpandHdVH (int player);      // transition from early pressure to a macro game
bool ZergMidPowerPureMutaHdVH (int player);                  // 2 base muta harassment, with ling+static defense
bool ZergMidPowerLingBaneMutaHdVH (int player);              // attack with ling+bane+muta together
